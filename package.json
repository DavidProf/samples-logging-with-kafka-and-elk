{
    "name": "samples-logging-with-kafka-and-elk",
    "version": "1.0.0",
    "description": "a simple sample to test logs with kafka & elk",
    "main": "src/index.ts",
    "devDependencies": {
        "@types/node": "^16.9.6",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    },
    "scripts": {
        "start:dev": "ts-node-dev --transpile-only src/index.ts",
        "test": "test",
        "compose:up": "docker-compose -f .docker/docker-compose.yml up -d",
        "compose:stop": "docker-compose -f .docker/docker-compose.yml stop",
        "compose:down": "docker-compose -f .docker/docker-compose.yml down -v"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DavidProf/samples-logging-with-kafka-and-elk.git"
    },
    "keywords": [
        "kafka",
        "elasticsearch",
        "logs",
        "logging",
        "elk"
    ],
    "author": "David Lima (Xarlyu)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DavidProf/samples-logging-with-kafka-and-elk/issues"
    },
    "homepage": "https://github.com/DavidProf/samples-logging-with-kafka-and-elk#readme",
    "dependencies": {
        "log4js": "^6.3.0",
        "winston": "^3.3.3"
    }
}
